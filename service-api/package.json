{
  "name": "service-api",
  "version": "0.43.1",
  "private": true,
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "standard": {
    "env": [
      "mocha",
      "describe"
    ]
  },
  "scripts": {
    "test": "mocha --timeout 10000 --exit",
    "test:awesome": "mocha --reporter mochawesome --exit || true",
    "test-coverage": "nyc --reporter=text mocha --exit",
    "test-coverage-html": "nyc --reporter=html mocha -timeout 10000 --exit",
    "test-awesome-coverage": "nyc --reporter=html mocha --reporter mochawesome --exit || true",
    "start": "npm run prod",
    "build": "npm-run-all clean transpile  && cp -r ./server/docs ./dist-server/docs/",
    "server": "node --max-old-space-size=1400 ./dist-server/app",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "copy": "cp server/middleware/*.conf dist-server/middlewares/",
    "watch:dev": "nodemon --ignore 'public/'",
    "eslint": "./node_modules/.bin/eslint server test",
    "eslint-fix": "./node_modules/.bin/eslint server test --fix",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate-debug": "node_modules/.bin/sequelize db:migrate --debug",
    "debug": "babel-node ./server/app.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "husky": {},
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "amazon-cognito-identity-js": "^5.2.3",
    "apminsight": "^2.2.2",
    "aws-amplify": "^3.3.7",
    "aws-sdk": "^2.756.0",
    "axios": "^1.7.2",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "body-parser": "^1.19.0",
    "bull": "^3.14.0",
    "bull-board": "^0.9.0",
    "casbin": "^5.0.4",
    "casbin-sequelize-adapter": "^2.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "cross-fetch": "^3.0.5",
    "crypto-js": "^4.0.0",
    "csv-validator": "0.0.3",
    "elasticsearch": "^16.7.1",
    "express": "^4.19.2",
    "express-validator": "^6.6.0",
    "express-winston": "^4.2.0",
    "fs": "0.0.1-security",
    "geoip-lite": "^1.4.2",
    "google-libphonenumber": "^3.2.31",
    "ioredis": "^4.17.3",
    "json-rules-engine": "^5.0.3",
    "jsonwebtoken": "^9.0.0",
    "jwk-to-pem": "^2.0.3",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.19",
    "logform": "^2.2.0",
    "mocha": "^7.2.0",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "mudder": "^1.1.1",
    "multer": "^1.4.2",
    "mustache": "^4.2.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.8",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "pg-promise": "^11.8.0",
    "prom-client": "^13.1.0",
    "read-excel-file": "^4.0.6",
    "redis": "^3.0.2",
    "redis-clustr": "^1.7.0",
    "rimraf": "^3.0.2",
    "save": "^2.4.0",
    "save-dev": "0.0.1-security",
    "sequelize": "^6.29.0",
    "sqs-producer": "^2.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-eslint-snake-case": "0.0.6",
    "eslint-plugin-import": "^2.21.1",
    "husky": "^4.2.5",
    "mochawesome": "^6.1.1",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.2.0"
  }
}